{
  "name": "md-editor-rt",
  "version": "2.1.2",
  "keywords": [
    "react",
    "nextjs",
    "javascript",
    "typescript",
    "jsx",
    "tsx",
    "ssr",
    "markdown",
    "editor",
    "theme",
    "html"
  ],
  "description": "Markdown editor for react, developed by jsx and typescript, dark theme、beautify content by prettier、render articles directly、paste or clip the picture and upload it...",
  "author": {
    "name": "zbf",
    "url": "https://imzbf.cc"
  },
  "homepage": "https://imzbf.github.io/md-editor-rt/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imzbf/md-editor-rt.git"
  },
  "bugs": {
    "url": "https://github.com/imzbf/md-editor-rt/issues"
  },
  "files": [
    "lib"
  ],
  "types": "./lib/MdEditor/Editor.d.ts",
  "module": "./lib/md-editor-rt.es.js",
  "exports": {
    ".": {
      "import": "./lib/md-editor-rt.es.js"
    },
    "./lib/style.css": "./lib/style.css"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode=preview"
  },
  "dependencies": {
    "dayjs": "^1.11.3",
    "md-editor-rt": "^2.1.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "redux": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@types/node": "^17.0.35",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^18.0.5",
    "@types/react-redux": "^7.1.24",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "axios": "^0.27.2",
    "eslint": "^8.16.0",
    "eslint-config-alloy": "^4.5.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "less": "^4.1.1",
    "multiparty": "^4.2.3",
    "prettier": "^2.6.2",
    "ts-morph": "^15.0.0",
    "typescript": "^4.7.2",
    "vite": "^2.9.9"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "@typescript-eslint",
      "react"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-empty-function": "off"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "semi": true,
    "printWidth": 90,
    "proseWrap": "never",
    "endOfLine": "auto"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
